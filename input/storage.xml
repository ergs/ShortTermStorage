<simulation>
  <control>
    <duration>130</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
    <decay>manual</decay>
    <dt>31557600</dt>
    <!--<solver><config><coin-or><timeout>100</timeout><verbose>1</verbose></coin-or></config></solver>-->
  </control>

  <archetypes>
    <spec> <lib>agents</lib> <name>NullInst</name></spec>
    <spec> <lib>agents</lib> <name>NullRegion</name></spec>
    <spec> <lib>cycamore</lib> <name>Source</name></spec>
    <spec> <lib>cycamore</lib> <name>Sink</name></spec>
    <spec> <lib>ShortTermStorage</lib> <name>ShortTermStorage</name></spec>
  </archetypes>

  <facility>
    <name>wetstorage</name>
    <config>
      <ShortTermStorage>
        <in_commod>spent</in_commod>
        <out_commod>spent2</out_commod>
        <decay_heat_ulimit>1.e100</decay_heat_ulimit>
      </ShortTermStorage>
    </config>
  </facility>

  <facility>
    <name>drystorage</name>
    <config>
      <ShortTermStorage>
        <in_commod>spent2</in_commod>
        <out_commod>spent3</out_commod>
        <decay_heat_ulimit>2.0e-6</decay_heat_ulimit>
      </ShortTermStorage>
    </config>
  </facility>

  <facility>
    <name>deepgeo</name>
    <config>
      <ShortTermStorage>
        <in_commod>spent3</in_commod>
        <out_commod>spent4</out_commod>
        <decay_heat_ulimit>4.2e-7</decay_heat_ulimit>
      </ShortTermStorage>
    </config>
  </facility>

  <facility>
    <name>source</name>
    <config>
      <Source>
        <outcommod>spent</outcommod>
        <outrecipe>spent_uox</outrecipe>
        <inventory_size>2.e5</inventory_size>
        <throughput>2.e4</throughput>
      </Source>
    </config>
  </facility>

  <region>
    <name>SingleRegion</name>
    <config><NullRegion/></config>
    <institution>
      <name>SingleInstitution</name>
      <initialfacilitylist>
        <entry>
          <prototype>wetstorage</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>drystorage</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>deepgeo</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>source</prototype>
          <number>1</number>
        </entry>
      </initialfacilitylist>
      <config><NullInst/></config>
    </institution>
  </region>

